{
    "builds": [
      {
        "src": "server/bin/www.js",
        "use": "@vercel/node"
      },
      {
        "src": "client/package.json",
        "use": "@vercel/static-build",
        "config": { "distDir": "build" }
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "server/bin/www.js"
      },
      {
        "src": "/(.*)",
        "dest": "client/build/$1"
      }
    ]
  }  